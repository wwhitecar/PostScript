/backgroundfill {
       newpath
       0 0 moveto
       640 0 lineto
       640 800 lineto
       0 800 lineto
       0 0 lineto
       closepath
       0 0 0 setrgbcolor fill


} def

/drawcircles {
	%Bottem left circle "purple"
	0 0 400 0 360 arc closepath
	1 .25 1 setrgbcolor fill

	%Bottem right circle "blue"
	640 0 200 0 360 arc closepath
	0 .75 1 setrgbcolor fill

	%Top left circle "yellow"
	150 300 400 0 360 arc closepath
	1 .5 0 setrgbcolor fill

	%Top right circle "green"
	640 800 400 0 360 arc closepath
	0 1 0 setrgbcolor fill
}def

/shape {
    gsave
	   1 1 3 {    
		   1  0  0  pstack setrgbcolor	   
		    250 350  translate
	    	    0 3 260 {
		    	newpath
		        dup
			dup		
		    	moveto
		    	rotate
		    	0 50 rlineto
		    	50 0 rlineto
		    	0 -50 rlineto
		    	-50 0 rlineto
		    	closepath
		    	fill
			stroke
			} for	
		} for		
    grestore

 gsave
	   1 1 3 {    
		   0 1 0 setrgbcolor
		    dup	   
		    150 450 translate
	    	    0 3 260 {
		    	newpath
		        dup
			dup		
		    	moveto
		    	rotate
		    	0 50 rlineto
		    	50 0 rlineto
		    	0 -50 rlineto
		    	-50 0 rlineto
		    	closepath
		    	fill
			stroke
			} for	
		} for		
    grestore

 gsave
	   1 1 3 {    
		   0 0 1 setrgbcolor	   
		    350 250  translate
	    	    0 3 260 {
		    	newpath
		        dup
			dup		
		    	moveto
		    	rotate
		    	0 50 rlineto
		    	50 0 rlineto
		    	0 -50 rlineto
		    	-50 0 rlineto
		    	closepath
		    	fill
			stroke
			} for	
		} for		
    grestore
} def

backgroundfill
%50 rotate
%drawcircles
shape
showpage

