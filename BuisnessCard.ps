% This function will draw a box that will fill the whole page
% with the color black. Then will draw a second box that will be
% filled white that will give us the black boarder around the card.
/CreateBoarder {

   %First Box to be filled with black
   newpath
   0 0 moveto
   612 0 rlineto
   0 792 rlineto
   -612 0 rlineto
   0 -792 rlineto
   fill
   closepath
   stroke

   %Second box to be filled with white
   newpath
   20 20 moveto 
   572 0 rlineto 
   0 752 rlineto 
   -572 0 rlineto 
   0 -752 rlineto
   1 1 1 setrgbcolor  
   fill
   closepath
   stroke
} def

/Innerboxes {
	newpath
	245 20 moveto
	347 0 rlineto
	0 379 rlineto
	-347 0 rlineto
	0 -379 rlineto
	closepath
	0 0 0 setrgbcolor
	fill
	
	newpath
	20 400 moveto
	225 0 rlineto
	0 379 rlineto
	-225 0 rlineto
	0 -379 rlineto
	closepath
	fill
	stroke
} def

/FirstW {
	%Set color to black for the W's
	0 0 0 setrgbcolor
	%Set font for the W's
	/Times-Roman findfont
	200 scalefont
	setfont
	
	%First W 
	190 253 moveto
        90 rotate
	(W) show
	270 rotate 	
	stroke
} def

/SecondW {
	%Set color to black for the W's
