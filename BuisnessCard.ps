% This function will draw a box that will fill the whole page
% with the color black. Then will draw a second box that will be
% filled white that will give us the black boarder around the card.
/CreateBoarder {
   %First Box
   newpath
   0 0 moveto
   612 0 rlineto
   0 792 rlineto
   -612 0 rlineto
   0 -792 rlineto
   fill
   closepath
   stroke

   %Second box
   newpath
   20 20 moveto 
   572 0 rlineto 
   0 752 rlineto 
   -572 0 rlineto 
   0 -752 rlineto
   1 1 1 setrgbcolor  
   fill
   closepath
   stroke
}def

/FirstW{
	%Set color to black for the W's
	0 0 0 setrgbcolor
	%Set font for the W's
	/Times-Roman findfont
	200 scalefont
	setfont
	
	%First W 
	200 245 moveto
        90 rotate
	(W) show
	270 rotate 	
	stroke
} def

